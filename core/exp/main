    %store/vec4 v0x55dd8bdc1680_0, 0, 4;
    %delay 10, 0;
    %load/vec4 v0x55dd8bdc1740_0;
    %cmpi/e 14, 0, 4;
    %jmp/0xz  T_0.2, 6;
    %jmp T_0.3;
T_0.2 ;
